		/>
		<property
			name="remark"
			column="REMARK"
			type="string"
			not-null="false"
			length="1000"
		/>
		<property
			name="createUserId"
			column="CREATE_USER_ID"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="createDate"
			column="CREATE_DATE"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="updateUserId"
			column="UPDATE_USER_ID"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="updateDate"
