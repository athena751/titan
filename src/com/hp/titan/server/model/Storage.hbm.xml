			name="storageName"
			column="STORAGE_NAME"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="storageState"
			column="STORAGE_STATE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="storageType"
			column="STORAGE_TYPE"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="storageStatus"
			column="STORAGE_STATUS"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="location"
			column="LOCATION"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="sn"
			column="SN"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="pn"
			column="PN"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="capacity"
			column="CAPACITY"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="slotNum"
			column="SLOT_NUM"
			type="string"
			not-null="false"
			length="100"
