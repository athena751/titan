			name="result"
			column="RESULT"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="remark"
			column="REMARK"
			type="string"
			not-null="false"
			length="1000"
		/>
		<property
			name="createUserId"
			column="CREATE_USER_ID"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="createDate"
			column="CREATE_DATE"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="UpdateUserId"
			column="UPDATE_USER_ID"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="updateDate"
			column="UPDATE_DATE"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="isValid"
			column="IS_VALID"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="str2"
			column="STR2"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="str3"
			column="STR3"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="str4"
			column="STR4"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Str5"
			column="STR5"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="str6"
			column="STR6"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="str7"
			column="STR7"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="integer1"
			column="INTEGER1"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="integer2"
			column="INTEGER2"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="integer3"
			column="INTEGER3"
			type="integer"
			not-null="false"
			length="11"
		/>
		 <many-to-one name="owner" class="com.hp.titan.auth.model.User" lazy="false" update="false" insert="false" fetch="select">
            <column name="CREATE_USER_ID">
                <comment>owner id</comment>
            </column>
          </many-to-one>

	</class>	
</hibernate-mapping>